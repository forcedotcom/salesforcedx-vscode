{
  "name": "@salesforce/salesforcedx-sobjects",
  "displayName": "Salesforce Faux SObject Generator",
  "description":
    "Fetches sobjects and generates their faux apex class to be used for Apex Language Server",
  "version": "40.8.0",
  "publisher": "salesforce",
  "license": "BSD-3-Clause",
  "engines": {
    "vscode": "^1.15.0"
  },
  "dependencies": {
    "@salesforce/salesforcedx-utils-vscode": "40.5.1",
    "jsforce": "1.7.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "2.2.38",
    "@types/node": "^6.0.40",
    "@types/sinon": "^2.3.2",
    "chai": "^4.0.2",
    "mocha": "3.2.0",
    "sinon": "^2.3.6",
    "nyc": "^11.0.2",
    "typescript": "2.4.0",
    "vscode": "1.1.4",
    "mock-spawn": "0.2.6",
    "cross-env": "5.0.4"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "tslint --project .",
    "watch": "tsc -watch -p .",
    "clean": "shx rm -rf node_modules && shx rm -rf out",
    "test":
      "./node_modules/.bin/cross-env VSCODE_NLS_CONFIG={} ./node_modules/.bin/nyc ./node_modules/.bin/_mocha --recursive out/test"
  },
  "nyc": {
    "reporter": ["text-summary", "lcov"]
  }
}
