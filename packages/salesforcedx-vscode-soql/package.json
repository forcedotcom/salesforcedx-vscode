{
  "name": "salesforcedx-vscode-soql",
  "displayName": "SOQL",
  "description": "Provides code-editing features for SOQL",
  "qna": "https://github.com/forcedotcom/salesforcedx-vscode/issues",
  "bugs": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode/issues"
  },
  "repository": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode"
  },
  "version": "49.7.0",
  "publisher": "salesforce",
  "license": "BSD-3-Clause",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "dependencies": {
    "@salesforce/soql-builder-ui": "0.0.2",
    "@salesforce/soql-language-server": "0.2.1",
    "vscode-languageclient": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "12.0.12",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "6.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "eslint-plugin-prettier": "^3.1.4",
    "typescript": "3.7.5"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint -c .eslintrc.json --ext .ts ./src",
    "watch": "tsc -watch -p .",
    "vscode:package": "vsce package"
  },
  "activationEvents": [
    "onCustomEditor:soqlCustom.soql",
    "onLanguage:soql"
  ],
  "main": "./out/src",
  "contributes": {
    "customEditors": [
      {
        "viewType": "soqlCustom.soql",
        "displayName": "SOQL Builder",
        "selector": [
          {
            "filenamePattern": "*.soql"
          }
        ],
        "priority": "default"
      }
    ],
    "languages": [
      {
        "id": "soql",
        "aliases": [
          "soql",
          "SOQL"
        ],
        "extensions": [
          ".soql"
        ]
      }
    ]
  }
}
