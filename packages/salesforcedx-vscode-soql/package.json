{
  "name": "salesforcedx-vscode-soql",
  "displayName": "SOQL",
  "description": "Provides code-editing features for SOQL",
  "qna": "https://github.com/forcedotcom/soql-tooling/issues",
  "bugs": {
    "url": "https://github.com/forcedotcom/soql-tooling/issues"
  },
  "repository": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode"
  },
  "version": "62.1.0",
  "publisher": "salesforce",
  "license": "BSD-3-Clause",
  "icon": "images/VSCodeSoql.png",
  "galleryBanner": {
    "color": "#ECECEC",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "soql",
    "salesforce",
    "salesforcedx",
    "sfdx",
    "soql builder"
  ],
  "dependencies": {
    "@jsforce/jsforce-node": "3.4.1",
    "@salesforce/apex-tmlanguage": "1.6.0",
    "@salesforce/core-bundle": "8.6.1",
    "@salesforce/soql-builder-ui": "1.0.0",
    "@salesforce/soql-data-view": "0.1.0",
    "@salesforce/soql-language-server": "0.7.1",
    "@salesforce/soql-model": "1.0.0"
  },
  "devDependencies": {
    "@salesforce/salesforcedx-sobjects-faux-generator": "62.1.0",
    "@salesforce/salesforcedx-test-utils-vscode": "62.1.0",
    "@salesforce/salesforcedx-utils-vscode": "62.1.0",
    "@salesforce/soql-common": "0.2.1",
    "@salesforce/ts-sinon": "1.4.0",
    "@salesforce/ts-types": "2.0.12",
    "@types/chai": "4.3.3",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^5",
    "@types/node": "^18.11.9",
    "@types/papaparse": "^5.2.3",
    "@types/sinon": "7.5.2",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "applicationinsights": "1.0.7",
    "chai": "^4.0.2",
    "cross-env": "5.2.0",
    "debounce": "^1.2.0",
    "esbuild": "^0.19.5",
    "esbuild-plugin-pino": "^2.1.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.5.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^8.1.0",
    "mocha": "^10",
    "papaparse": "^5.3.0",
    "prettier": "3.0.3",
    "sinon": "13.0.1",
    "typescript": "^5.2.2",
    "vscode-extension-tester": "^5.0.0",
    "vscode-languageclient": "6.1.3"
  },
  "extensionDependencies": [
    "salesforce.salesforcedx-vscode-core"
  ],
  "scripts": {
    "bundle:extension": "node ./esbuild.config.js",
    "build": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "watch": "tsc -watch -p .",
    "clean": "shx rm -rf node_modules && shx rm -rf out && shx rm -rf coverage && shx rm -rf .nyc_output",
    "copy:files": "shx cp -R ../../node_modules/@salesforce/apex-tmlanguage/grammars .",
    "test": "npm run test:vscode-integration",
    "test:ui:prepare": "extest get-vscode -s ~/.vscode-test-resources && extest get-chromedriver -s ~/.vscode-test-resources ",
    "test:ui:install": "extest install-vsix -f ./salesforcedx-vscode-soql-*.vsix -s ~/.vscode-test-resources -e test/ui-test/resources/extensions ",
    "test:ui:run": "extest run-tests -s ~/.vscode-test-resources -o test/ui-test/resources/settings.json -e test/ui-test/resources/extensions out/test/ui-test/*.test.js",
    "test:ui": "npm run test:ui:prepare && npm run test:ui:install && npm run test:ui:run",
    "test:unit": "jest --coverage",
    "test:vscode-integration": "node ../../scripts/run-vscode-integration-tests-with-top-level-extensions",
    "test:vscode-insiders-integration": "cross-env CODE_VERSION=insiders npm run test:vscode-integration",
    "vscode:package": "ts-node  ../../scripts/vsce-bundled-extension.ts",
    "vscode:sha256": "node ../../scripts/generate-sha256.js >> ../../SHA256",
    "vscode:publish": "node ../../scripts/publish-vsix.js"
  },
  "activationEvents": [
    "workspaceContains:sfdx-project.json"
  ],
  "main": "out/src",
  "serverPath": [
    "..",
    "..",
    "node_modules",
    "@salesforce",
    "soql-language-server",
    "lib",
    "server.js"
  ],
  "soqlBuilderWebAssetsPath": [
    "..",
    "..",
    "node_modules",
    "@salesforce",
    "soql-builder-ui"
  ],
  "packaging": {
    "assets": [
      "LICENSE.txt",
      "package.nls.ja.json",
      "package.nls.json",
      "README.md",
      ".vscodeignore",
      "grammars",
      "images",
      "dist"
    ],
    "packageUpdates": {
      "main": "dist/index.js",
      "serverPath": [
        "node_modules",
        "@salesforce",
        "soql-language-server",
        "lib",
        "server.js"
      ],
      "soqlBuilderWebAssetsPath": [
        "node_modules",
        "@salesforce",
        "soql-builder-ui"
      ],
      "dependencies": {
        "applicationinsights": "1.0.7",
        "@salesforce/apex-tmlanguage": "1.6.0",
        "shelljs": "0.8.5",
        "@salesforce/soql-builder-ui": "1.0.0",
        "@salesforce/soql-data-view": "0.1.0",
        "@salesforce/soql-language-server": "0.7.1",
        "@salesforce/soql-model": "1.0.0"
      },
      "devDependencies": {}
    }
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "soqlCustom.soql",
        "displayName": "%soqlCustom_soql%",
        "selector": [
          {
            "filenamePattern": "*.soql"
          }
        ],
        "priority": "option"
      }
    ],
    "languages": [
      {
        "id": "soql",
        "aliases": [
          "soql",
          "SOQL"
        ],
        "extensions": [
          ".soql"
        ]
      }
    ],
    "grammars": [
      {
        "language": "soql",
        "scopeName": "source.soql",
        "path": "./grammars/soql.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "soql.builder.open.new",
        "title": "%soql_builder_open_new%"
      },
      {
        "command": "soql.builder.toggle",
        "title": "%soql_builder_toggle%",
        "icon": "$(go-to-file)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "soql.builder.open.new",
          "when": "sf:project_opened"
        },
        {
          "command": "soql.builder.toggle",
          "when": "false"
        }
      ],
      "editor/title": [
        {
          "when": "resourceExtname == .soql",
          "command": "soql.builder.toggle",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "%configuration_title%",
      "properties": {
        "salesforcedx-vscode-soql.experimental.validateQueries": {
          "type": "boolean",
          "default": false,
          "description": "%soql_validation_flag%"
        }
      }
    }
  }
}
