{
  "name": "salesforcedx-vscode-services",
  "displayName": "Salesforce Services",
  "description": "a shared services extension for other extensions to use",
  "qna": "https://github.com/forcedotcom/salesforcedx-vscode/issues",
  "bugs": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode/issues"
  },
  "repository": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode"
  },
  "icon": "images/VSCodeBundle.png",
  "galleryBanner": {
    "color": "#ECECEC",
    "theme": "light"
  },
  "version": "65.10.0",
  "publisher": "salesforce",
  "license": "BSD-3-Clause",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "dependencies": {
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-beta.32",
    "@effect/opentelemetry": "0.59.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.208.0",
    "@opentelemetry/resources": "2.0.1",
    "@opentelemetry/sdk-logs": "0.203.0",
    "@opentelemetry/sdk-metrics": "2.0.1",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "@opentelemetry/sdk-trace-node": "2.0.1",
    "@opentelemetry/sdk-trace-web": "2.2.0",
    "@salesforce/core": "^8.23.4",
    "@salesforce/source-deploy-retrieve": "^12.29.1",
    "@salesforce/source-tracking": "^7.8.0",
    "@vscode/extension-telemetry": "^1.0.0",
    "effect": "^3.19.11",
    "jsforce": "^3.10.2",
    "vscode-uri": "^3.1.0"
  },
  "devDependencies": {
    "@effect/platform": "0.93.7",
    "@salesforce/playwright-vscode-ext": "*",
    "browserify-zlib": "^0.2.0",
    "esbuild": "0.25.0",
    "https-browserify": "^1.0.0",
    "jsforce": "^3.9.4",
    "nyc": "^15",
    "querystring-es3": "^0.2.1",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12"
  },
  "scripts": {
    "vscode:bundle": "wireit",
    "vscode:package": "wireit",
    "vscode:sha256": "node ../../scripts/generate-sha256.js >> ../../SHA256",
    "vscode:publish": "node ../../scripts/publish-vsix.js",
    "compile": "wireit",
    "generate-types": "wireit",
    "lint": "wireit",
    "clean": "shx rm -rf node_modules out dist coverage .nyc_output .wireit .eslintcache *.vsix",
    "test": "wireit",
    "run:web": "wireit",
    "test:web": "wireit",
    "test:web:ui": "npm run test:web -- --headed",
    "test:web:debug": "npm run test:web -- --debug",
    "test:e2e": "wireit"
  },
  "wireit": {
    "compile": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "dependencies": [
        "generate-types"
      ],
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "tsconfig.json",
        "../../tsconfig.common.json"
      ],
      "output": [
        "out/**",
        "tsconfig.tsbuildinfo"
      ]
    },
    "generate-types": {
      "command": "ts-node ../salesforcedx-vscode-services-types/scripts/generateEntry.ts && ts-node ../salesforcedx-vscode-services-types/scripts/syncDeps.ts",
      "files": [
        "src/**/*.ts",
        "../salesforcedx-vscode-services-types/scripts/*.ts"
      ],
      "output": []
    },
    "vscode:bundle": {
      "command": "node esbuild.config.mjs",
      "dependencies": [
        "compile"
      ],
      "files": [
        "esbuild.config.mjs",
        "../../scripts/bundling/node.mjs",
        "../../scripts/bundling/web.mjs",
        "out/**"
      ],
      "output": [
        "dist"
      ]
    },
    "test": {
      "command": "jest --coverage",
      "dependencies": [
        "compile"
      ],
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "jest.config.js",
        "../../config/jest.base.config.js"
      ],
      "output": [
        "coverage"
      ]
    },
    "lint": {
      "command": "eslint --color --cache --cache-location .eslintcache .",
      "dependencies": [
        "../eslint-local-rules:compile"
      ],
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        "../../eslint.config.mjs",
        "package.json",
        "package.nls.json"
      ],
      "output": []
    },
    "vscode:package": {
      "command": "vsce package --allow-package-all-secrets",
      "dependencies": [
        "vscode:bundle"
      ],
      "files": [
        "package.json",
        "package.nls*.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.txt",
        "images/**"
      ],
      "output": [
        "*.vsix"
      ]
    },
    "test:web": {
      "command": "playwright test --config=playwright.config.web.ts",
      "dependencies": [
        "vscode:bundle",
        "../playwright-vscode-ext:compile"
      ],
      "files": [
        "playwright.config.web.ts"
      ]
    },
    "test:e2e": {
      "dependencies": [
        "test:web"
      ]
    },
    "run:web": {
      "command": "npx vscode-test-web --browserType=chromium --browserOption=--disable-web-security --browserOption=--remote-debugging-port=9222 --extensionDevelopmentPath . --open-devtools --port 3001",
      "service": true,
      "dependencies": [
        "vscode:bundle"
      ],
      "files": [
        "test/playwright/**/*.ts"
      ]
    }
  },
  "activationEvents": [
    "*"
  ],
  "main": "dist/index.js",
  "types": "out/src/index.d.ts",
  "browser": "./dist/web/index.js",
  "contributes": {
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "salesforcedx-vscode-code-builder-web.instanceUrl": {
          "type": "string",
          "default": "",
          "description": "%configuration.instanceUrl.description%"
        },
        "salesforcedx-vscode-code-builder-web.accessToken": {
          "type": "string",
          "default": "",
          "description": "%configuration.accessToken.description%"
        },
        "salesforcedx-vscode-code-builder-web.apiVersion": {
          "type": "string",
          "default": "64.0",
          "description": "%configuration.apiVersion.description%"
        },
        "salesforcedx-vscode-code-builder-web.retrieveOnLoad": {
          "type": "string",
          "default": "",
          "description": "%configuration.retrieveOnLoad.description%"
        },
        "salesforcedx-vscode-salesforcedx.enableLocalTraces": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.otelLocal.description%"
        },
        "salesforcedx-vscode-salesforcedx.enableConsoleTraces": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.otelConsole.description%"
        }
      }
    },
    "commands": []
  },
  "sideEffects": true
}
