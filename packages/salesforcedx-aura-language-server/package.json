{
  "name": "@salesforce/salesforcedx-aura-language-server",
  "displayName": "Aura Language Server",
  "version": "64.16.1",
  "description": "Language server for Aura components.",
  "publisher": "salesforce",
  "engines": {
    "vscode": "^1.90.0"
  },
  "main": "out/src/index.js",
  "typings": "out/src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./out/src/index.js",
      "require": "./out/src/index.js",
      "types": "./out/src/index.d.ts"
    },
    "./utils/componentUtil": {
      "import": "./out/src/util/componentUtil.js",
      "require": "./out/src/util/componentUtil.js",
      "types": "./out/src/util/componentUtil.d.ts"
    }
  },
  "license": "BSD-3-Clause",
  "repository": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode"
  },
  "scripts": {
    "clean": "shx rm -rf node_modules && shx rm -rf out && shx rm -rf coverage",
    "compile": "tsc -p . && node scripts/build.js",
    "watch": "tsc -watch -p .",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@salesforce/salesforcedx-lightning-lsp-common": "64.16.1",
    "@salesforce/salesforcedx-utils": "65.3.1",
    "acorn-loose": "^6.0.0",
    "acorn-walk": "^6.0.0",
    "line-column": "^1.0.2",
    "@types/vscode": "^1.90.0",
    "vscode-html-languageservice": "^5.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-languageserver-types": "3.17.5",
    "vscode-uri": "1.0.6"
  },
  "devDependencies": {
    "jest-extended": "^0.11.5"
  }
}
