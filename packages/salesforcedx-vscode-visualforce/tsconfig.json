{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "dom",
            "es6"
        ],
        "sourceMap": true,
        "declaration": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "rootDir": ".",
        "outDir": "out",
        "preserveConstEnums": true,
        "strict": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test",
        "out"
    ]
}