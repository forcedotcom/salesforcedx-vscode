{
  "name": "@salesforce/vscode-services",
  "version": "65.15.2",
  "description": "TypeScript type definitions for Salesforce VS Code Services extension API",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/forcedotcom/salesforcedx-vscode"
  },
  "bugs": {
    "url": "https://github.com/forcedotcom/salesforcedx-vscode/issues"
  },
  "homepage": "https://github.com/forcedotcom/salesforcedx-vscode/tree/develop/packages/salesforcedx-vscode-services-types",
  "types": "out/salesforcedx-vscode-services-types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./out/salesforcedx-vscode-services-types/src/index.d.ts"
    }
  },
  "files": [
    "out/**/*.d.ts",
    "README.md",
    "LICENSE.txt"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate-entry": "ts-node scripts/generateEntry.ts",
    "sync-deps": "ts-node scripts/syncDeps.ts",
    "compile": "wireit",
    "clean": "shx rm -rf src out dist node_modules .wireit .eslintcache *.tgz"
  },
  "wireit": {
    "compile": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "dependencies": [
        "../salesforcedx-vscode-services:generate-types"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../tsconfig.common.json"
      ],
      "output": [
        "out/**",
        "tsconfig.tsbuildinfo"
      ]
    }
  },
  "dependencies": {
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-beta.32",
    "@effect/opentelemetry": "0.59.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.209.0",
    "@opentelemetry/sdk-logs": "0.203.0",
    "@opentelemetry/sdk-metrics": "2.5.0",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "@opentelemetry/sdk-trace-node": "2.2.0",
    "@opentelemetry/sdk-trace-web": "2.2.0",
    "@salesforce/core": "^8.23.3",
    "@salesforce/source-deploy-retrieve": "^12.25.0",
    "@salesforce/source-tracking": "^7.8.0",
    "@types/vscode": "^1.90.0",
    "effect": "^3.19.13",
    "jsforce": "^3.10.2",
    "vscode-uri": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
