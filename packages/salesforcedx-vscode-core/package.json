      {
        "id": "ignore",
        "filenames": [
          ".forceignore"
        ]
      },
      {
        "id": "json",
        "filenamePatterns": [
          "*.wdash",
          "*.wdf",
          "*.wdpr",
          "*.wlens",
          "*.orchestration"
        ]
      },
      {
        "id": "forcesourcemanifest",
        "aliases": [
          "ForceSourceManifest",
          "forcesourcemanifest"
        ],
        "filenamePatterns": [
          "**/manifest/**/*.xml"
        ]
      }
    ],
    "grammars": [
      {
        "language": "forcesourcemanifest",
        "scopeName": "manifest.text.xml",
        "path": "./syntaxes/manifestXML.tmLanguage.json"
      }
    ]
