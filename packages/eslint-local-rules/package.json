{
  "name": "@salesforce/eslint-plugin-vscode-extensions",
  "version": "65.7.0",
  "description": "Custom ESLint rules for Salesforce VSCode extensions",
  "main": "out/index.js",
  "files": [
    "out/**/*.js",
    "out/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "lint": "eslint .",
    "clean": "shx rm -rf out"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "salesforce"
  ],
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.0.0",
    "@types/estree": "^1.0.0",
    "@typescript-eslint/rule-tester": "^8.47.0",
    "@typescript-eslint/utils": "^8.47.0"
  }
}
