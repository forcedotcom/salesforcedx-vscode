{
  "extends": "@salesforce/dev-config/tslint",
  "rules": {
    "no-reserved-keywords": false,
    "no-any": false,
    "quotemark": [true, "single", "avoid-escape"],
    "member-ordering": false,
    "no-string-based-set-timeout": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  }
}
